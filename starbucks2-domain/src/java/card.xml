<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ns.card">

    <select id="list" resultType="CARD">
        SELECT * FROM sb_card WHERE uid = #{value}
    </select>

    <insert id="create" parameterType="CARD">
        INSERT INTO sb_card (number,code,balance,date_added,uid)
        VALUES(#{number},#{code},#{balance},#{date_added},#{uid});
    </insert>

</mapper>