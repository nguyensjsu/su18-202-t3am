<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ns.purchase">

    <select id="list" resultType="PURCHASE">
        SELECT * FROM sb_purchase WHERE uid = #{value}
    </select>

    <insert id="create" parameterType="PURCHASE">
        INSERT INTO sb_purchase (uid, email, change, note, date_committed)
        VALUES(#{uid}, #{email}, #{change}, #{note}, #{date_committed});
    </insert>

</mapper>
